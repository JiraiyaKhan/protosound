{
  "manifest_version": 2,
  "name": "ProtoSound",
  "version": "0.1"
  
  "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
  }
  
  "permissions": [
	"*://*.google.com/", "webRequest"
  ]
  
  "content_scripts": [
  {
	"matches": ["http://www.google.com/*"]
	
	"js": ["protosound.js"]
  }
  
}
