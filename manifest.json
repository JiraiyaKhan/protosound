{
  "manifest_version": 2,
  "name": "ProtoSound",
  "version": "0.2",
  
  "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  
  "permissions": [
	"webRequest","*://*.google.com/"
  ],
  
  "content_scripts": [
  	{
	"matches": ["http://www.google.com/*"],
	"js": ["protosound.js"]
  	}
  
  ]
}
